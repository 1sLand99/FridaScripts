{
  "scripts": {
    "build": "esbuild --bundle --outfile=hook.js src/main.ts",
    "build_minify": "esbuild --bundle --minify --outfile=hook.js src/main.ts",
    "watch": "npm run build_minify -- --watch",
    "frida": "adb shell su -c ./data/local/tmp/fs",
    "frida_stop": "adb shell su -c pkill -9 fs",
    "hook": "frida -Ul hook.js -f com.xqc.sdklink",
    "hook_o": "frida -Ul hook.js -f com.xqc.sdklink -o 1.tx",
    "hook_example": "frida -Ul hook.js -f com.example.app"
  },
  "dependencies": {
    "@types/frida-gum": "^18.4.0",
    "esbuild": "^0.23.1",
    "frida-il2cpp-bridge": "^0.9.0"
  }
}
